{
  "name": "dslink-dart-webctrl",
  "version": "1.1.5",
  "description": "WebCtrl DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.13.0"
  },
  "getDependencies": [
    "pub get"
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "WebCtrl"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum",
      "default": "info"
    }
  }
}
